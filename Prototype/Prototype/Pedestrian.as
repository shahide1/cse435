package  {	import flash.display.MovieClip	import Speed;	import Position;		public class Pedestrian extends MovieClip{		public var pos:Position;		public var velocity:Speed;		public var arrived:Boolean;				public function Pedestrian() {			// constructor code			pos = new Position();			velocity = new Speed();			pos.setXPos(0);			pos.setYPos(0);			velocity.setXVel(0);			velocity.setYVel(0);			velocity.setAngle(0);			arrived = false;		}					}}