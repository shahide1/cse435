package  {		public class Speed {				private var x_vel:Number;		private var y_vel:Number;		private var total_vel:Number;		private var angle:Number;		public function Speed() {			// constructor code		}				//X Velocity		public function getXVel():Number {			return x_vel;		}		public function setXVel(num:Number):void {			x_vel = num;			total_vel = Math.sqrt(Math.pow(x_vel, 2) + Math.pow(y_vel, 2));		}				//Y Velocity		public function getYVel():Number {			return y_vel;		}		public function setYVel(num:Number):void {			y_vel = num;			total_vel = Math.sqrt(Math.pow(x_vel, 2) + Math.pow(y_vel, 2));		}				//Total Velocity		public function getTotalVel():Number {			return total_vel;		}				//Angle		public function getAngleRads():Number {			return angle;		}		public function getAngleDegs():Number {			return (angle * (180 / Math.PI));		}		public function setAngle(rads:Number):void {			angle = rads;		}	}	}